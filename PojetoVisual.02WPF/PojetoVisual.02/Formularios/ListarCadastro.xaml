<Window x:Class="PojetoVisual._02.Formularios.ListarCadastro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PojetoVisual._02.Formularios"
        mc:Ignorable="d"
        Title="ListarCadastro" Height="450" Width="800">
    <Grid Height="NaN" Margin="0,0,0,-6" Width="NaN" Background="#FFC2EDEC" AutomationProperties.Name="">
        <Label Content="LISTA DE CADASTROS" HorizontalAlignment="Center" Margin="0,37,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="20" FontWeight="Bold" FontStyle="Italic"/>
        <DataGrid x:Name="Lista" d:ItemsSource="{d:SampleData ItemCount=5}" Height="NaN" Margin="85,91,85,28" Width="NaN" AutomationProperties.Name="dgLista"/>

    </Grid>
</Window>
